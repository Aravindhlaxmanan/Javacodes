package aravindh;
import java.io.*;

public class String_reverse {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String name="aravind";
		StringBuilder result = Reverse(name);
		System.out.println(result);
		

	}

	private static StringBuilder Reverse(String name) {
		
		StringBuilder sb = new StringBuilder(name);
		sb.reverse().toString();
		return sb;
	}
	
	

	

}
